/* pages/order_make/order_make.wxss */
@import "/pages/icon/icon.wxss";
page{
  background-color: #f8f8f8;
}
.receive{
  width:100%;
  height:130rpx;
  position: relative;
  border: 5rpx solid pink;
  border-image-source: url(data:image/png;base64,);
  border-image-slice: 0 0 20 0;
  border-image-repeat: round;
}
.icon-weibiaoti2fuzhi08{
  position: absolute;
  top:20rpx;
  left: 30rpx;
  font-size:36rpx;
}
.receive-icon{
  top:30rpx;
  font-size: 54rpx;
  color:#666;
}
.receive-description{
  position: absolute;
  left: 120rpx;
  top: 40rpx;
}
.receive-name{
  position: absolute;
  top:20rpx;
  left:80rpx;
  font-size:28rpx;
}
.receive-phone{
  position: absolute;
  top:30rpx;
  right:60rpx;
  font-size:28rpx;
}
.receive-address{
  position: absolute;
  bottom:30rpx;
  left:80rpx;
  font-size:24rpx;
  color:#666;
}
.navigator-arrow {
  position: absolute;
  top:50%;
  right:-10rpx;
  height: 100%;
  margin-top:-65rpx;
}
.navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #888888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -9rpx;
  right: 1rem;  
}

/* 订单主体 */
.order-container{
  width:100%;
  background-color: #f8f8f8;
  margin-bottom: 100rpx;
}
.order-item{
  width:100%;
  margin-top: 20rpx;
}
.order-top{
  width:100%;
  height:70rpx;
  background-color: #fff;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.stop{
  font-size:28rpx;
  line-height: 70rpx;
  color:#000;
}
.order-middle{
  margin-top:10rpx;
  width:100%;
}
.order-product{
  width:100%;
  height:200rpx;
  position: relative;
  background-color: #f8f8f8;
}
.product-img{
  position: absolute;
  top:10rpx;
  left: 20rpx;
  width:150rpx;
  height:150rpx;
}
.product-title{
  display:block;
  position: absolute;
  top:10rpx;
  left:190rpx;
  width:50%;
  height:180rpx;
  overflow: hidden;
  font-size:32rpx;
  color:#000;
}
.product-price{
  display:block;
  position: absolute;
  top:10rpx;
  right:20rpx;
  font-size:28rpx;
  color:#000;
}
.product-num{
  display:block;
  position: absolute;
  top:90rpx;
  right:20rpx;
  font-size:28rpx;
  color:#aaa;
}
.order-bottom{
  width:100%;
}
.order-bottom-item{
  width:100%;
  height:90rpx;
  line-height: 70rpx;
  padding:10rpx 20rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.order-bottom-item-left{
  float:left;
  font-size:28rpx;
}
.order-bottom-item-right{
  float:right;
  font-size:28rpx;
  color:#333;
}
.distribution{
  height:110rpx;
  line-height: 90rpx;
  border-bottom:1px solid #f8f8f8;
}
.distribution .order-bottom-item-right{
  line-height: 45rpx;
  text-align: right;
}
.offer{
  margin:20rpx 0;
}

/* 底部悬浮栏 */
.bottom-bar{
  position:fixed;
  width:100%;
  bottom:0;
  height:100rpx;
  background-color: #fff;
}
.total{
  display: inline-block;
  width:65%;
  line-height: 100rpx;
  font-size:28rpx;
  padding:0 20rpx;
  box-sizing: border-box;
  text-align: right;
}
.total-price{
  color:#f00;
}
.buy-now{
  display:inline-block;
  width:35%;
  height:100%;
  line-height: 100rpx;
  font-size:32rpx;
  font-weight: bold;
  text-align: center;
}
.buy-now{
  color:#fff;
  background-color: #D3B579;
}